{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vnetName": {
      "type": "string",
      "defaultValue": "VNet",
      "metadata": {
        "description": "VNet name"
      }
    },
    "vnetAddressPrefix": {
      "type": "string",
      "defaultValue": "10.0.0.0/16",
      "metadata": {
        "description": "Address prefix"
      }
    },
    "subnet1Prefix": {
      "type": "string",
      "allowedValues": [
        "10.0.1.0/24",
        "10.0.2.0/24"
      ],
      "defaultValue": "10.0.1.0/24",
      "metadata": {
        "description": "Subnet 1 Prefix"
      }
    },
    "subnet1Name": {
      "type": "string",
      "maxLength": 9,
      "defaultValue": "public",
      "metadata": {
        "description": "Subnet 1 Name"
      }
    },
    "subnet2Prefix": {
      "type": "string",
      "allowedValues": [
        "10.0.3.0/24",
        "10.0.4.0/24"
      ],
      "defaultValue": "10.0.3.0/24",
      "metadata": {
        "description": "Subnet 2 Prefix"
      }
    },
    "subnet2Name": {
      "type": "string",
      "maxLength": 9,
      "defaultValue": "private",
      "metadata": {
        "description": "Subnet 2 Name"
      }
    }
  },
  "variables": {
    "subnetName": "[concat(toLower(parameters('subnet1Name')), uniqueString('vnetName().id'))]",
    "subnetName2": "[concat(toLower(parameters('subnet2Name')), uniqueString('vnetName().id'))]",
    "templateBaseUrl": "https://rawgit.com/serush46/Azure/master/linkedtemplates/",
    "secondTemplate" : "[concat(variables('templateBaseUrl'),'child-2vm's.json')]"    },
  "resources": [
    {
      "apiVersion": "2015-06-15",
      "type": "Microsoft.Network/virtualNetworks",
      "name": "[parameters('vnetName')]",
      "location": "[resourceGroup().location]",
      "properties": {
        "addressSpace": {
          "addressPrefixes": [
            "[parameters('vnetAddressPrefix')]"
          ]
        },
        "dhcpOptions": {
          "dnsServers": [
            "8.8.8.8"
          ]
        },
        "subnets": [
          {
            "name": "[variables('subnetName')]",
            "properties": {
              "addressPrefix": "[parameters('subnet1Prefix')]"
            }
          },
          {
            "name": "[variables('subnetName2')]",
            "properties": {
              "addressPrefix": "[parameters('subnet2Prefix')]"
            }
          }
        ]
      }
    },
    {
      "type": "Microsoft.Resources/deployments",
      "name": "WebServer",
      "apiVersion": "2017-05-10",
      "dependsOn": [
        "[concat('Microsoft.Network/virtualNetworks/', variables('subnetName2'))]",
        "[concat('Microsoft.Network/virtualNetworks/', parameters('vnetName'))]"
      ],
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(variables('secondTemplate'))]",
          "contentVersion": "1.0.0.0"
        }
      }
    }
  ],
  "outputs": {
    "virtualnet": {
      "type": "string",
      "value": "[parameters('vnetName')]"
    },
    "subnetname": {
      "type": "string",
      "value": "[variables('subnetName2')]"
    }
  }
}
